{
  "name": "modern-express",
  "version": "1.0.0",
  "description": "Easy starter project for writing modern Express Applications using TypeScript",
  "private": true,
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "node node_modules/mocha/bin/mocha ./build/tests/*.js",
    "build": "node ./config/build.js",
    "postinstall": "node node_modules/typings/dist/bin.js install --overwrite",
    "start": "DEBUG=modern-express:* node build/bin/www.js"
  },
  "main": "",
  "keywords": [
    "Easy",
    "TypeScript",
    "Starter",
    "Template"
  ],
  "author": "Rick Hernandez",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.3"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "app-root-path": "^1.3.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "morgan": "^1.7.0"
  }
}
